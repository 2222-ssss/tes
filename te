local function MPNAHN_script() -- Btn7.7.scr 
	local script = Instance.new('Script', Btn7)

	local antivoid = false
	local 7b = false
	local fallHeightLimit = -math.huge
	
	Btn7.MouseButton1Click:Connect(function()
		7b = not 7b
		if 7b then
			c7.BackgroundColor3 = Color3.fromRGB(26, 159, 35)
			antivoid = true
	
			game:GetService("RunService").Heartbeat:Connect(function()
				if antivoid then
					if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
						local humanoidRootPart = plr.Character.HumanoidRootPart
						if humanoidRootPart.Position.Y < fallHeightLimit then
							humanoidRootPart.Velocity = Vector3.new(0, 0, 0)
							plr.Character.Humanoid.Health = plr.Character.Humanoid.Health
						end
					end
				end
			end)
	
		else
			c7.BackgroundColor3 = Color3.fromRGB(159, 0, 0)
			antivoid = false
		end
	end)
	
end
coroutine.wrap(MPNAHN_script)()
