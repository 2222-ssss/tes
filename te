-- Scripts:

local function CUYIOU_script() -- Toggle.Script 
	local script = Instance.new('Script', Toggle)

	local opened = true
	
	function close()
		if opened == true then
			for i,v in pairs(script.Parent.Parent.Container:GetChildren()) do
				if v:IsA("GuiObject") then
					v.Visible = false
				end
			end
	
			script.Parent.Parent.Container:TweenSize(UDim2.new(0, 620, 0, 0))
	
			repeat
				wait(0.1)
			until script.Parent.Parent.Container.Size.Y == UDim.new(0,0)
	
			script.Parent.Text = ""
			opened = false
		elseif opened == false then
			script.Parent.Parent.Container:TweenSize(UDim2.new(0, 620, 0, 360))
	
			repeat
				wait(0.1)
			until script.Parent.Parent.Container.Size.Y == UDim.new(0, 360)
	
			for i,v in pairs(script.Parent.Parent.Container:GetChildren()) do
				if v:IsA("GuiObject") then
					v.Visible = true
				end
			end
	
			script.Parent.Text = ""
			opened = true
		end
	end
	
	script.Parent.MouseButton1Click:Connect(close)
	
end
coroutine.wrap(CUYIOU_script)()
local function YXZUK_script() -- Btn1.1.scr 
	local script = Instance.new('Script', Btn1)

	local antifling = false
	local connection
	
	Btn1.MouseButton1Click:Connect(function()
		antifling = not antifling
		if antifling then
			c1.BackgroundColor3 = Color3.fromRGB(26, 159, 35)
			local player = game.Players.LocalPlayer
			local character = player.Character
	
			connection = workspace.DescendantAdded:Connect(function(child)
				pcall(function()
					if child.Name == 'Stabler' then
						if child:WaitForChild('ConnectedWeld', 10).Part1:IsDescendantOf(player.Character) then
							child:Destroy()
						end
					end
				end)
			end)
	
			local function resetArm()
				while wait() do
					if character and player.Character == character then
						local normalState = CFrame.new(0.999999583, 0.650959253, 0.0507530943, 0.994099438, 0.0974220186, 0.0476996154, -0.0791255683, 0.952069402, -0.295470744, -0.074198693, 0.289953023, 0.954160273)
						for _, v in ipairs(character:GetDescendants()) do
							if v:IsA('BasePart') and v.Name ~= 'HumanoidRootPart' then
								v.Massless = true
							end
						end
						character.ArmAngleChange:FireServer(normalState)
					else
						break
					end
				end
			end
	
			local function onCharacterAdded(newCharacter)
				character = newCharacter
				resetArm()
			end
	
			player.CharacterAdded:Connect(onCharacterAdded)
		else
			c1.BackgroundColor3 = Color3.fromRGB(159, 0, 0)
			if connection then
				connection:Disconnect()
			end
		end
	end)
	
end
coroutine.wrap(YXZUK_script)()
local function AODNI_script() -- Btn2.2.scr 
	local script = Instance.new('Script', Btn2)

	local 2b = false
	local antiragdoll = false
	
	Btn2.MouseButton1Click:Connect(function()
		2b = not 2b
		antiragdoll = not antiragdoll
	
		if 2b then
			c2.BackgroundColor3 = Color3.fromRGB(26, 159, 35)
	
			game:GetService("RunService").Heartbeat:Connect(function()
				pcall(function()
					if plr.Character and plr.Character:FindFirstChild("Ragdoll") then
						if plr.Character.Ragdoll.Value and antiragdoll then
							plr.Character.GetUpEvent:FireServer()
						end
					end
				end)
			end)
	
		else
			c2.BackgroundColor3 = Color3.fromRGB(159, 0, 0)
			antiragdoll = false
		end
	end)
	
end
coroutine.wrap(AODNI_script)()
local function UFGBJ_script() -- Btn3.3.scr 
	local script = Instance.new('Script', Btn3)

	local antifalldamage = false
	local 3b = false
	
	Btn3.MouseButton1Click:Connect(function()
		3b = not 3b
		if 3b then
			c3.BackgroundColor3 = Color3.fromRGB(26, 159, 35)
			antifalldamage = true
	
			game:GetService("RunService").Heartbeat:Connect(function()
				if antifalldamage then
					pcall(function()
						for _, _2 in pairs(workspace:GetChildren()) do
							if _2 and _2.Parent == plr.Character then
								if _2.Name == 'VelocityDamage' or _2.Name == 'GetPicked' then
									_2:Destroy()
								end
							end
						end
					end)
				end
			end)
		else
			c3.BackgroundColor3 = Color3.fromRGB(159, 0, 0)
			antifalldamage = false
		end
	end)
	
end
coroutine.wrap(UFGBJ_script)()
local function BWFZKOZ_script() -- Btn4.4.scr 
	local script = Instance.new('Script', Btn4)

	local antispinner = false
	local 4b = false
	local prop1000 = PhysicalProperties.new(1000, 1000, 0, 1000, 0)
	
	Btn4.MouseButton1Click:Connect(function()
		4b = not 4b
		if 4b then
			c4.BackgroundColor3 = Color3.fromRGB(26, 159, 35)
	
			repeat wait() until plr.Character:FindFirstChild("HumanoidRootPart") and plr.Character:FindFirstChild("Head")
	
			for _, v in next, plr.Character:GetChildren() do
				if v:IsA("BasePart") then
					v.CustomPhysicalProperties = prop1000
				end
			end
	
			antispinner = true
		else
			c4.BackgroundColor3 = Color3.fromRGB(159, 0, 0)
			antispinner = false
		end
	end)
	
end
coroutine.wrap(BWFZKOZ_script)()
